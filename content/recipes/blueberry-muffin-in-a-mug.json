  "recipeIngredient": [
    "1/3 c flour (all purpose)",
    "1 tbsp brown sugar",
    "2.5 tbsp sugar",
    "1/4 tsp baking powder",
    "2 tbsp butter (salted, melted)",
    "1/2 tsp vanilla",
    "1/4 c milk (2% or whole)",
    "1/4 c blueberries (fresh is best)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a small dish melt your butter in the microwave for 30 seconds."
    },
    {
      "@type": "HowToStep",
      "text": "In your mug stir together the flour, sugar, brown sugar, and baking powder well."
    },
    {
      "@type": "HowToStep",
      "text": "Pour in your melted butter, milk, and vanilla and stir so you have batter and no powder is left."
    },
    {
      "@type": "HowToStep",
      "text": "Gently fold in your fresh blueberries."
    },
    {
      "@type": "HowToStep",
      "text": "Put into the microwave for 2 minutes. Eat immediately for best results."
    }
  ],
