  "recipeIngredient": [
    "8 c. low-sodium chicken or vegetable broth",
    "1 tbsp. extra-virgin olive oil",
    "1 onion, finely chopped",
    "2 tbsp. butter, divided",
    "2 cloves garlic, minced",
    "1 lb. button mushrooms, sliced",
    "1 bay leaf",
    "4 sprigs thyme, leaves removed",
    "kosher salt",
    "Freshly ground black pepper",
    "2 c. arborio rice",
    "1/2 c. white wine",
    "1 c. freshly grated Parmesan",
    "3/4 c. frozen peas, thawed",
    "2 tbsp. chopped fresh parsley"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a medium saucepan over medium heat, bring chicken broth to a simmer."
    },
    {
      "@type": "HowToStep",
      "text": "Reduce heat to low."
    },
    {
      "@type": "HowToStep",
      "text": "In a large pot or Dutch oven, heat oil."
    },
    {
      "@type": "HowToStep",
      "text": "Add onion and cook, stirring often, until translucent, about 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add 1 tablespoon butter, garlic, mushrooms, bay leaf and thyme."
    },
    {
      "@type": "HowToStep",
      "text": "Cook until the mushrooms have softened and are golden, about 4 more minutes, then season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Remove mixture from the pot."
    },
    {
      "@type": "HowToStep",
      "text": "Melt remaining tablespoon butter in the pot and add the arborio rice, stirring quickly."
    },
    {
      "@type": "HowToStep",
      "text": "Cook until the grains are well-coated and smell slightly toasty, about 2 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add the wine and cook until the wine has mostly absorbed."
    },
    {
      "@type": "HowToStep",
      "text": "With a ladle, add about 1 cup hot broth."
    },
    {
      "@type": "HowToStep",
      "text": "Stirring often, cook until the rice has mostly absorbed liquid."
    },
    {
      "@type": "HowToStep",
      "text": "Add remaining broth about 1 cup at a time, continuing to allow the rice to absorb each addition of broth before adding more."
    },
    {
      "@type": "HowToStep",
      "text": "Stir often and cook until the risotto is al dente and creamy, not mushy."
    },
    {
      "@type": "HowToStep",
      "text": "(You might not need all the broth."
    },
    {
      "@type": "HowToStep",
      "text": ") Add the mushroom mixture back into the rice."
    },
    {
      "@type": "HowToStep",
      "text": "Stir in Parmesan and peas then garnish with parsley."
    },
    {
      "@type": "HowToStep",
      "text": "Serve warm."
    }
  ],
  "recipeYield": "4 servings",
