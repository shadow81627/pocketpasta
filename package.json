{
  "name": "pocketpasta",
  "version": "0.0.7",
  "description": "PocketPasta is a collection of my demo apps that I build to practice my skills.",
  "author": "Damien Robinson <damien.robinson@pocketpasta.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "test": "jest && codecov",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.4.1",
    "@nuxtjs/google-analytics": "2.2.0",
    "@nuxtjs/pwa": "2.6.0",
    "@nuxtjs/sentry": "2.3.1",
    "bootstrap-vue": "2.0.0-rc.15",
    "cross-env": "5.2.0",
    "koa": "2.7.0",
    "nuxt": "2.4.5",
    "vuetify": "1.5.7",
    "vuetify-loader": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@nuxtjs/eslint-config": "0.0.1",
    "@storybook/addon-actions": "5.0.3",
    "@storybook/addon-links": "5.0.3",
    "@storybook/addons": "5.0.3",
    "@storybook/vue": "5.0.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "babel-loader": "8.0.5",
    "babel-preset-vue": "2.0.2",
    "codecov": "3.2.0",
    "eslint": "5.15.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-nuxt": "0.4.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "jest": "24.5.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "vue-jest": "3.0.4",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2"
  }
}
