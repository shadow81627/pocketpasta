{
  "name": "pocketpasta",
  "version": "0.46.28",
  "description": "Eat spagetti to forgetti your regretti",
  "author": {
    "name": "Damien Robinson",
    "email": "damien.robinson@pocketpasta.com",
    "url": "https://pocketpasta.com"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/shadow81627/pocketpasta.git"
  },
  "bugs": "https://github.com/shadow81627/pocketpasta/issues",
  "homepage": "https://pocketpasta.com",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "nuxt build --modern",
    "build:analyze": "npm run build -- --analyze",
    "build:export": "npm run build && npm run generate",
    "serve": "cross-env HOST=localhost PORT=3001 nuxt serve",
    "serve:lhci": "npm run serve",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "cross-env HOST=localhost PORT=3001 nuxt export --modern",
    "build:modern": "nuxt build --modern",
    "start:modern": "nuxt start --modern",
    "generate:modern": "cross-env HOST=localhost PORT=3001 nuxt export --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:fix": "eslint --ext .js,.vue, --ignore-path .gitignore . --fix",
    "format": "prettier --write \"**/*.+(json|css|md)\" --ignore-path .gitignore",
    "lhci:autorun": "cross-env FIREBASE_HOSTING=false lhci autorun",
    "test": "jest && codecov",
    "test:update": "jest -u",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "git-cz": "git-cz",
    "now-build": "nuxt build --modern && nuxt export --modern",
    "stylelint:fix": "npx stylelint **/*.css !**/node_modules/** !**/dist/** !**/coverage/** --fix",
    "scrape": "node scrape && npm run format",
    "scrape:new": "node scrape --only-new && npm run format",
    "scrape:dry": "node scrape --no-scrape && npm run format"
  },
